{
  "name": "ariad/ExactTargetLaravel",
  "description": "Ariad customization to The best way to talk to Exact Target with Hypertext PreProcessor",
  "license": "MIT",
  "keywords": [
    "ariad-exacttarget-laravel"
  ],
  "homepage": "https://github.com/:vendor/:package_name",
  "authors": [
    {
      "name": "Patrick Bennett",
      "email": "patrick.bennett@digitaladditive.com",
      "homepage": "http://digitaladditive.com",
      "role": "Developer"
    },
    {
      "name": "Julio Rodriguez",
      "email": "jrodriguez@ariad.ca",
      "homepage": "http://ariad.ca",
      "role": "Developer"
    }
  ],
  "require": {
    "illuminate/support": "~5.0",
    "php": "~5.5|~7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "5.0.*",
    "scrutinizer/ocular": "~1.1",
    "squizlabs/php_codesniffer": "~2.3",
    "guzzlehttp/guzzle": "~6.0",
    "psr/http-message": "1.0",
    "guzzlehttp/psr7": "1.0.0",
    "laravel/homestead": "^3.0"
  },
  "autoload": {
    "classmap": [
      "src/"
    ],
    "psr-4": {
      "ariad\\ExactTargetLaravel\\": "src"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/",
      "src/"
    ],
    "psr-4": {
      "digitaladditive\\ExactTargetLaravel\\Test\\": "tests"
    }
  },
  "scripts": {
    "test": "phpunit"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  }
}
